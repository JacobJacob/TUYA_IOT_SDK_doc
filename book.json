{
  "plugins": [
    "js-sequence-diagram-full@>=0.3.1",
    "prism",
    "-highlight",
    "anchors",
    "plantuml",
    "page-copyright",
    "flexible-alerts",
    "pageview-count",
    "popup",
    "chapter-fold",
    "splitter",
    "page-treeview",
    "advanced-emoji",
    "todo",
    "back-to-top-button",
    "copy-code-button"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ],
      "lang": {
        "java": "java"
      },
      "ignore": [
        "sequence"
      ]
    },
    "page-copyright": {
      "description": "Modified at",
      "signature": "<a href='https://github.com/TuyaInc' target='_blank'>Tuya Inc</a>",
      "wisdom": "<a href='https://www.tuya.com' target='_blank'>www.tuya.com</a>",
      "format": "YYYY-MM-dd hh:mm:ss",
      "copyright": "Copyright &#169; Tuya Inc",
      "timeColor": "#666",
      "copyrightColor": "#666",
      "utcOffset": "8",
      "style": "normal",
      "noPowered": true,
      "isShowQRCode": false
    }
  }
}